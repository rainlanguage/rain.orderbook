---
source: crates/subgraph/tests/batch_order_test.rs
expression: request_body.query
---
query BatchOrderDetailQuery($id_list: Order_filter!) {
  orders(orderBy: lastModified, orderDirection: desc, where: $id_list) {
    id
    orderBytes
    orderHash
    owner
    outputs {
      id
      token {
        id
        address
        name
        symbol
        decimals
      }
      balance
      vaultId
    }
    inputs {
      id
      token {
        id
        address
        name
        symbol
        decimals
      }
      balance
      vaultId
    }
    active
    addEvents {
      transaction {
        blockNumber
        timestamp
      }
    }
    meta
    timestampAdded
    orderbook {
      id
    }
    lastModified
  }
}
