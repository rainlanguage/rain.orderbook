---
source: crates/subgraph/tests/order_test.rs
expression: request_body.query
---
query OrderDetailQuery($id: ID!) {
  order(id: $id) {
    id
    orderBytes
    orderHash
    owner
    outputs {
      id
      token {
        id
        address
        name
        symbol
        decimals
      }
      balance
      vaultId
    }
    inputs {
      id
      token {
        id
        address
        name
        symbol
        decimals
      }
      balance
      vaultId
    }
    active
    addEvents {
      transaction {
        blockNumber
        timestamp
      }
    }
    meta
    timestampAdded
    orderbook {
      id
    }
  }
}
