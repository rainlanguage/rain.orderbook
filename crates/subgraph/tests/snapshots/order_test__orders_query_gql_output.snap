            }
          }
          timestamp
          transaction {
            id
            from
            blockNumber
            timestamp
          }
          orderbook {
            id
          }
        }
        ... on Deposit {
          id
          __typename
          amount
          newVaultBalance
          oldVaultBalance
          vault {
            id
            token {
              id
              address
              name
              symbol
              decimals
            }
          }
          timestamp
          transaction {
            id
            from
            blockNumber
            timestamp
          }
          orderbook {
            id
          }
        }
      }
    }
    orderbook {
      id
    }
    active
    timestampAdded
    meta
    addEvents {
      transaction {
        id
        from
        blockNumber
        timestamp
      }
    }
    trades {
      id
    }
  }
