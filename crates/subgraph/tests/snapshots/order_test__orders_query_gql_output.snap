query OrderDetailQuery($id: ID!) {
  order(id: $id) {
    id
    orderBytes
    orderHash
    owner
    outputs {
      id
      owner
      vaultId
      balance
      token {
        id
        address
        name
        symbol
        decimals
      }
      orderbook {
        id
      }
      ordersAsOutput {
        id
        orderHash
        active
      }
      ordersAsInput {
        id
        orderHash
        active
      }
      balanceChanges {
        __typename
        ... on Withdrawal {
          id
          __typename
          amount
          newVaultBalance
          oldVaultBalance
          vault {
            id
            token {
              id
              address
              name
              symbol
              decimals
            }
          }
          timestamp
          transaction {
            id
            from
            blockNumber
            timestamp
          }
          orderbook {
            id
          }
        }
        ... on TradeVaultBalanceChange {
          id
          __typename
          amount
          newVaultBalance
          oldVaultBalance
          vault {
            id
            token {
              id
              address
              name
              symbol
              decimals
            }
          }
          timestamp
          transaction {
            id
            from
            blockNumber
            timestamp
          }
          orderbook {
            id
          }
        }
        ... on Deposit {
          id
          __typename
          amount
          newVaultBalance
          oldVaultBalance
          vault {
            id
            token {
              id
              address
              name
              symbol
              decimals
            }
          }
          timestamp
          transaction {
            id
            from
            blockNumber
            timestamp
          }
          orderbook {
            id
          }
        }
      }
    }
    inputs {
      id
      owner
      vaultId
      balance
      token {
        id
        address
        name
        symbol
        decimals
      }
      orderbook {
        id
      }
      ordersAsOutput {
        id
        orderHash
        active
      }
      ordersAsInput {
        id
        orderHash
        active
      }
      balanceChanges {
        __typename
        ... on Withdrawal {
          id
          __typename
          amount
          newVaultBalance
          oldVaultBalance
          vault {
            id
            token {
              id
              address
              name
              symbol
              decimals
            }
          }
          timestamp
          transaction {
            id
            from
            blockNumber
            timestamp
          }
          orderbook {
            id
          }
        }
        ... on TradeVaultBalanceChange {
          id
          __typename
          amount
          newVaultBalance
          oldVaultBalance
          vault {
            id
            token {
              id
