---
source: crates/subgraph/tests/orders_test.rs
expression: request_body.query
---
query OrdersListQuery($first: Int, $skip: Int) {
  orders(orderBy: lastModified, orderDirection: desc, skip: $skip, first: $first) {
    id
    orderBytes
    orderHash
    owner
    outputs {
      token {
        id
        address
        name
        symbol
        decimals
      }
      balance
      vaultId
    }
    inputs {
      token {
        id
        address
        name
        symbol
        decimals
      }
      balance
      vaultId
    }
    active
    addEvents {
      transaction {
        id
        blockNumber
        timestamp
      }
    }
    timestampAdded
    orderbook {
      id
    }
    lastModified
  }
}
