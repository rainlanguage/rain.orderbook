query VaultDetailQuery($id: ID!) {
  vault(id: $id) {
    vaultId
    token {
      id
      address
      name
      symbol
      decimals
    }
    owner
    ordersAsOutput {
      id
      orderHash
      active
    }
    ordersAsInput {
      id
      orderHash
      active
    }
    balanceChanges {
      __typename
      ... on Withdrawal {
        id
        __typename
        amount
        oldVaultBalance
        newVaultBalance
      }
      ... on TradeVaultBalanceChange {
        id
        __typename
        amount
        oldVaultBalance
        newVaultBalance
      }
      ... on Deposit {
        id
        __typename
        amount
        oldVaultBalance
        newVaultBalance
      }
    }
    balance
  }
}
