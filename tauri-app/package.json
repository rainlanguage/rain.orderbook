{
  "name": "tauri-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --fix src",
    "test": "vitest run",
    "lint-check": "eslint src",
    "format": "prettier --write src",
    "format-check": "prettier --list-different src",
    "svelte-lint-format-check": "npm run check && npm run lint-check && npm run format-check"
  },
  "devDependencies": {
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.1.1",
    "tslib": "^2.4.1",
    "vite-plugin-checker": "^0.6.2"
  },
  "type": "module",
  "dependencies": {
    "@codemirror/lang-yaml": "^6.0.0",
    "@fontsource/dm-sans": "^5.0.18",
    "@imask/svelte": "^7.3.0",
    "@observablehq/plot": "^0.6.13",
    "@sentry/sveltekit": "^7.107.0",
    "@tauri-apps/api": "^1.5.3",
    "@walletconnect/ethereum-provider": "^2.11.3",
    "@walletconnect/modal": "^2.6.2",
    "@web3modal/ethers5": "^4.1.1",
    "camelcase-keys": "^9.1.3",
    "codemirror-rainlang": "^3.0.11",
    "ethers": "^5.7.2",
    "flowbite-svelte": "^0.44.21",
    "fuse.js": "^7.0.0",
    "install": "^0.13.0",
    "lightweight-charts": "^4.1.3",
    "npm": "^10.6.0",
    "svelte-codemirror-editor": "^1.3.0",
    "svelte-markdown": "^0.4.1",
    "thememirror": "^2.0.1",
    "uuid": "^9.0.1"
  }
